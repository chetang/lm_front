<h1> All Users </h1>
<table class="table table-striped table-bordered">
	<tr>
		<th>Id</th>
		<th>Account</th>
		<th>Admin</th>
		<th>Name</th>
		<th>Roles</th>
		<th>State</th>
		<th>Created At</th>
		<th>Updated At</th>
	</tr>
	{{#each user in content}}
		<tr>
			<td>{{user.id}}</td>
			<td>{{#linkTo 'account' user.account}}{{user.account.name}}{{/linkTo}}</td>
			<td>{{user.admin}}</td>
			<td>{{#linkTo 'user' user}}{{user.name}}{{/linkTo}}</td>
			<td>{{user.modifiedRoles}}</td>
			<td>{{user.state}}</td>
			<td>{{user.created_at}}</td>
			<td>{{user.updated_at}}</td>
		</tr>
	{{/each}}
</table>