<h1> All Accounts </h1>
<table class="table table-striped table-bordered">
	<tr>
		<th>Id</th>
		<th>Name</th>
		<th>Users</th>
		<th>State</th>
		<th>Created At</th>
		<th>Updated At</th>
	</tr>
	{{#each account in content}}
		<tr>
			<td>{{account.id}}</td>
			<td>{{#linkTo 'account' account}}{{account.name}}{{/linkTo}}</td>
			<td>
				{{#each user in account.users}}		
					{{#linkTo 'user' user}}{{user.name}}{{/linkTo}} ,
				{{/each}}
			</td>
			<td>{{account.state}}</td>
			<td>{{account.created_at}}</td>
			<td>{{account.updated_at}}</td>
		</tr>
	{{/each}}
</table>